      "targets": [
        {
          "dataset": "sales_db",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH date_series AS (\r\n    SELECT generate_series(\r\n        (SELECT MIN(order_date)::date FROM sales_data),\r\n        (SELECT MAX(order_date)::date FROM sales_data),\r\n        interval '1 day'\r\n    ) AS day\r\n)\r\nSELECT \r\n    ds.day,\r\n    COALESCE(d.disputed_orders, 0) AS disputed_orders\r\nFROM date_series ds\r\nLEFT JOIN (\r\n    SELECT order_date::date AS day, COUNT(*) AS disputed_orders\r\n    FROM sales_data\r\n    WHERE status = 'Disputed'\r\n    GROUP BY day\r\n) d ON ds.day = d.day\r\nORDER BY ds.day;",
          "refId": "A",
          "sql": {
